[
  {
    "type": "PropSchema",
    "schema": {
      "type": "object",
      "key": "schema",
      "label": "Uploader 文件上传",
      "properties": [
        {
          "label": "设置Uploader 文件上传",
          "key": "tabs",
          "type": "object",
          "properties": [
            {
              "key": "value",
              "bindProp": "value",
              "type": "array",
              "label": "Uploader 文件上传 已上传的文件列表",
              "items": {
                "label": "菜单项",
                "key": "label",
                "type": "object",
                "properties": [
                  {
                    "type": "string",
                    "key": "imageFit",
                    "label": "类型",
                    "defaultValue": "cover",
                    "enumList": [
                      {
                        "label": "覆盖",
                        "value": "cover"
                      },
                      {
                        "label": "包含",
                        "value": "contain"
                      },
                      {
                        "label": "填充",
                        "value": "fill"
                      },
                      {
                        "label": "无",
                        "value": "none"
                      },
                      {
                        "label": "降低",
                        "value": "scale-down"
                      }
                    ]
                  },
                  {
                    "type": "img",
                    "key": "url",
                    "label": "图标地址",
                    "defaultValue": "https://img01.yzcdn.cn/vant/leaf.jpg"
                  },
                  {
                    "type": "deletable",
                    "key": "subtext",
                    "defaultValue": true,
                    "label": "是否可删除"
                  }
                ]
              }
            },
            {
              "key": "accept",
              "bindProp": "accept",
              "label": "允许上传的文件类型， 详细说明",
              "type": "string"
            },
            {
              "key": "name",
              "bindProp": "name",
              "label": "标识符，可以在回调函数的第二项参数中获取",
              "type": "string"
            },
            {
              "key": "preview-size",
              "bindProp": "preview-size",
              "label": "预览图和上传区域的尺寸，默认单位为  px",
