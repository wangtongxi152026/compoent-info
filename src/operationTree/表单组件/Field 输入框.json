[
  {
    "type": "PropSchema",
    "schema": {
      "type": "object",
      "key": "schema",
      "label": "输入框",
      "properties": [
        {
          "label": "设置输入框",
          "key": "tabs",
          "type": "object",
          "properties": [
            {
              "key": "value",
              "bindProp": "value",
              "label": "当前输入的值",
              "type": "string"
            },
            {
              "key": "label",
              "bindProp": "label",
              "label": "输入框左侧文本",
              "type": "string"
            },
            {
              "key": "name",
              "bindProp": "name",
              "label": "名称，提交表单的标识符",
              "type": "string"
            },
            {
              "key": "type",
              "bindProp": "type",
              "label": "输入框类型, 可选值为  tel digit number textarea password  等",
              "type": "string",
              "enumList": [
                {
                  "value": "tel",
                  "label": "tel"
                },
                {
                  "value": "digit",
                  "label": "digit"
                },
                {
                  "value": "number",
                  "label": "number"
                },
                {
                  "value": "textarea",
                  "label": "textarea"
                },
                {
                  "value": "password",
                  "label": "password"
                }
              ]
            },
            {
              "key": "size",
              "bindProp": "size",
              "label": "大小",
              "type": "string",
              "enumList": [
                {
                  "value": "large",
                  "label": "large"
                }
              ]
            },
            {
              "key": "maxlength",
              "bindProp": "maxlength",
              "label": "输入的最大字符数",
              "type": "string"
            },
            {
              "key": "placeholder",
              "bindProp": "placeholder",
              "label": "输入框占位提示文字",
              "type": "string"
            },
            {
              "key": "border",
              "bindProp": "border",
              "label": "是否显示内边框",
              "type": "boolean"
            },
            {
              "key": "disabled",
              "bindProp": "disabled",
              "label": "是否禁用输入框",
              "type": "boolean"
            },
            {
              "key": "readonly",
              "bindProp": "readonly",
              "label": "是否只读",
              "type": "boolean"
            },
            {
              "key": "colon",
              "bindProp": "colon",
              "label": "是否在 label 后面添加冒号",
              "type": "boolean"
            },
            {
              "key": "required",
              "bindProp": "required",
              "label": "是否显示表单必填星号",
              "type": "boolean"
            },
            {
              "key": "center",
              "bindProp": "center",
              "label": "是否使内容垂直居中",
              "type": "boolean"
            },
            {
              "key": "clearable",
              "bindProp": "clearable",
              "label": "是否启用清除图标，点击清除图标后会清空输入框",
              "type": "boolean"
            },
            {
              "key": "clear-trigger",
              "bindProp": "clear-trigger",
              "label": "显示清除图标的时机， always  表示输入框不为空时展示， focus  表示输入框聚焦且不为空时展示",
              "type": "string"
            },
            {
              "key": "clickable",
              "bindProp": "clickable",
              "label": "是否开启点击反馈",
              "type": "boolean"
            },
            {
              "key": "is-link",
              "bindProp": "is-link",
              "label": "是否展示右侧箭头并开启点击反馈",
              "type": "boolean"
            },
            {
              "key": "autofocus",
              "bindProp": "autofocus",
              "label": "是否自动聚焦，iOS 系统不支持该属性",
              "type": "boolean"
            },
            {
              "key": "show-word-limit",
              "bindProp": "show-word-limit",
              "label": "是否显示字数统计，需要设置 maxlength 属性",
              "type": "boolean"
            },
            {
              "key": "error",
              "bindProp": "error",
              "label": "是否将输入内容标红",
              "type": "boolean"
            },
            {
              "key": "error-message",
              "bindProp": "error-message",
              "label": "底部错误提示文案，为空时不展示",
              "type": "string"
            },
            {
              "key": "format-trigger",
              "bindProp": "format-trigger",
              "label": "格式化函数触发的时机",
              "type": "string",
              "enumList": [
                {
                  "value": "onBlur",
                  "label": "onBlur"
                },
                {
                  "value": "onChange",
                  "label": "onChange"
                }
              ]
            },
            {
              "key": "arrow-direction",
              "bindProp": "arrow-direction",
              "label": "箭头方向",
              "type": "string",
              "enumList": [
                {
                  "value": "left",
                  "label": "left"
                },
                {
                  "value": "up",
                  "label": "up"
                },
                {
                  "value": "down",
                  "label": "down"
                },
                {
                  "value": "right",
                  "label": "right"
                }
              ]
            },
            {
              "key": "label-width",
              "bindProp": "label-width",
              "label": "左侧文本宽度，默认单位为 px",
              "type": "string"
            },
            {
              "key": "label-align",
              "bindProp": "label-align",
              "label": "左侧文本对齐方式",
              "type": "string",
              "enumList": [
                {
                  "value": "center",
                  "label": "center"
                },
                {
                  "value": "right",
                  "label": "right"
                },
                {
                  "value": "left",
                  "label": "left"
                }
              ]
            },
            {
              "key": "input-align",
              "bindProp": "input-align",
              "label": "输入框对齐方式",
              "type": "string",
              "enumList": [
                {
                  "value": "center",
                  "label": "center"
                },
                {
                  "value": "right",
                  "label": "right"
                },
                {
                  "value": "left",
                  "label": "left"
                }
              ]
            },
            {
              "key": "error-message-align",
              "bindProp": "error-message-align",
              "label": "错误提示文案对齐方式",
              "type": "string",
              "enumList": [
                {
                  "value": "center",
                  "label": "center"
                },
                {
                  "value": "right",
                  "label": "right"
                },
                {
                  "value": "left",
                  "label": "left"
                }
              ]
            },
            {
              "key": "autosize",
              "bindProp": "autosize",
              "label": "是否自适应内容高度，只对textarea有效",
              "type": "boolean"
            },
            {
              "key": "left-icon",
              "bindProp": "left-icon",
              "label": "左侧 图标名称 或图片链接",
              "type": "img"
            },
            {
              "key": "right-icon",
              "bindProp": "right-icon",
              "label": "右侧 图标名称 或图片链接",
              "type": "img"
            }
          ]
        }
      ]
    },
    "label": "输入框",
    "render_tree_node": {
      "ins_id": "",
      "key": "schema"
    },
    "code_tree_node": {
      "ins_id": "",
      "key": "schema"
    }
  }
]