{
  "List 列表": {
    "minthods": [
      {
        "key": "v-model",
        "label": "List 列表 是否处于加载状态，加载过程中不触发 load 事件",
        "type": "boolean",
        "default": "false"
      },
      {
        "key": "finished",
        "label": "是否已加载完成，加载完成后不再触发 load 事件",
        "type": "boolean",
        "default": "false"
      },
      {
        "key": "error",
        "label": "是否加载失败，加载失败后点击错误提示可以重新 触发 load 事件，必须使用 sync 修饰符",
        "type": "boolean",
        "default": "false"
      },
      {
        "key": "offset",
        "label": "滚动条与底部距离小于 offset 时触发 load 事件",
        "type": "number | string",
        "default": "300"
      },
      {
        "key": "loading-text",
        "label": "加载过程中的提示文案",
        "type": "string",
        "default": "加载中..."
      },
      {
        "key": "finished-text",
        "label": "加载完成后的提示文案",
        "type": "string",
        "default": "-"
      },
      {
        "key": "error-text",
        "label": "加载失败后的提示文案",
        "type": "string",
        "default": "-"
      },
      {
        "key": "immediate-check",
        "label": "是否在初始化时立即执行滚动位置检查",
        "type": "boolean",
        "default": "true"
      },
      {
        "key": "direction",
        "label": "滚动触发加载的方向，可选值为 up",
        "type": "string",
        "default": "down",
        "enumList": [
          {
            "value": "down",
            "label": "down"
          }
        ]
      }
    ],
    "envets": [
      {
        "eventKey": "load",
        "label": "滚动条与底部距离小于 offset 时触发",
        "param": "- "
      }
    ],
    "slots": [
      {
        "slot": "default",
        "label": "列表内容"
      },
      {
        "slot": "loading",
        "label": "自定义底部加载中提示"
      },
      {
        "slot": "finished",
        "label": "自定义加载完成后的提示文案"
      },
      {
        "slot": "error",
        "label": "自定义加载失败后的提示文案"
      }
    ]
  }
}