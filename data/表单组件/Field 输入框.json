        "default": "left",
        "enumList": [
          {
            "value": "center",
            "label": "center"
          },
          {
            "value": "right",
            "label": "right"
          },
          {
            "value": "left",
            "label": "left"
          }
        ]
      },
      {
        "key": "autosize",
