{
  "Image 图片": {
    "prop": [
      {
        "key": "src",
        "label": "Image 图片 图片链接",
        "type": "string",
        "default": "-"
      },
      {
        "key": "fit",
        "label": "图片填充模式",
        "type": "string",
        "default": "fill"
      },
      {
        "key": "alt",
        "label": "替代文本",
        "type": "string",
        "default": "-"
      },
      {
        "key": "width",
        "label": "宽度，默认单位为 px",
        "type": "number | string",
        "default": "-"
      },
      {
        "key": "height",
        "label": "高度，默认单位为 px",
        "type": "number | string",
        "default": "-"
      },
      {
        "key": "radius",
        "label": "圆角大小，默认单位为 px",
        "type": "number | string",
        "default": "0"
      },
      {
        "key": "round",
        "label": "是否显示为圆形",
        "type": "boolean",
        "default": "false"
      },
      {
        "key": "lazy-load",
        "label": "是否开启图片懒加载，须配合  Lazyload  组件使用",
        "type": "boolean",
        "default": "false"
      },
      {
        "key": "show-error",
        "label": "是否展示图片加载失败提示",
        "type": "boolean",
        "default": "true"
      },
      {
        "key": "show-loading",
        "label": "是否展示图片加载中提示",
        "type": "boolean",
        "default": "true"
      },
      {
        "key": "error-icon",
        "label": "失败时提示的 图标名称 或图片链接",
        "type": "string",
        "default": "photo-fail"
      },
      {
        "key": "loading-icon",
        "label": "加载时提示的 图标名称 或图片链接",
        "type": "string",
        "default": "photo"
      },
      {
        "key": "icon-prefix ",
        "label": "图标类名前缀，同 Icon 组件的  class-prefix 属性",
        "type": "string",
        "default": "van-icon"
      }
    ],
    "envets": [
      {
        "eventKey": "click",
        "label": "点击图片时触发",
        "param": "event: Event "
      },
      {
        "eventKey": "load",
        "label": "图片加载完毕时触发",
        "param": "- "
      },
      {
        "eventKey": "error",
        "label": "图片加载失败时触发",
        "param": "- "
      }
    ],
    "slots": [
      {
        "slot": "default ",
        "label": "自定义图片下方的内容"
      },
      {
        "slot": "loading",
        "label": "自定义加载中的提示内容"
      },
      {
        "slot": "error",
        "label": "自定义加载失败时的提示内容"
      }
    ]
  }
}