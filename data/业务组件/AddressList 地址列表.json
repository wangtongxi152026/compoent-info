{
  "AddressList 地址列表": {
    "minthods": [
      {
        "key": "v-model",
        "label": "AddressList 地址列表当前选中地址的 id",
        "type": "string",
        "default": "-"
      },
      {
        "key": "list",
        "label": "地址列表",
        "type": "Address[]",
        "default": "[]"
      },
      {
        "key": "disabled-list",
        "label": "不可配送地址列表",
        "type": "Address[]",
        "default": "[]"
      },
      {
        "key": "disabled-text",
        "label": "不可配送提示文案",
        "type": "string",
        "default": "-"
      },
      {
        "key": "switchable",
        "label": "是否允许切换地址",
        "type": "boolean",
        "default": "true"
      },
      {
        "key": "add-button-text",
        "label": "底部按钮文字",
        "type": "string",
        "default": "新增地址"
      },
      {
        "key": "default-tag-text",
        "label": "默认地址标签文字",
        "type": "string",
        "default": "-"
      }
    ],
    "envets": [
      {
        "eventKey": "add",
        "label": "点击新增按钮时触发",
        "param": "-"
      },
      {
        "eventKey": "edit",
        "label": "点击编辑按钮时触发",
        "param": "item: 地址对象，index: 索引"
      },
      {
        "eventKey": "select",
        "label": "切换选中的地址时触发",
        "param": "item: 地址对象，index: 索引"
      },
      {
        "eventKey": "edit-disabled",
        "label": "编辑不可配送的地址时触发",
        "param": "item: 地址对象，index: 索引"
      },
      {
        "eventKey": "select-disabled",
        "label": "选中不可配送的地址时触发",
        "param": "item: 地址对象，index: 索引"
      },
      {
        "eventKey": "click-item",
        "label": "点击任意地址时触发",
        "param": "item: 地址对象，index: 索引"
      }
    ],
    "slots": [
      {
        "slot": "default",
        "label": "在列表下方插入内容"
      },
      {
        "slot": "top",
        "label": "在顶部插入内容"
      },
      {
        "slot": "item-bottom ",
        "label": "在列表项底部插入内容"
      }
    ]
  }
}
